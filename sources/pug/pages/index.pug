extends  ../_skeleton

//- Includes
include ../components/_box-info
include ../components/_box-product
include ../components/_post
include ../components/_box-minimal-horizontal
include ../components/_box-team
include ../components/_ui-thumbnail-classic
include ../components/_ui-pagination

block variables
  - var activeCategory = 'home'
  - var pageName = 'Home'

block page
  // Page Header
  include ../sections/_header-classic

  // Swiper
  section.section.swiper-container.swiper-slider.swiper-slider-1.snow-animate(data-loop='true' data-autoplay='4000' data-simulate-touch="false" data-slide-effect='fade')
    .swiper-wrapper.text-center
      .swiper-slide.bg-overlay-50.context-dark(data-slide-bg='images/slide-1.jpg')
        .swiper-slide-caption.section-md
          .container
            .row.justify-content-lg-center
              .col-lg-9
                p.slide-title(data-caption-animate='fadeInDown' data-caption-delay='250') Awesome
                h2.text-decoration-lines-2(data-caption-animate='fadeIn' data-caption-delay='50')
                  span Snowboarding experience
                    span.text-decoration-line.text-decoration-line-left(data-caption-animate='widthLines' data-caption-delay='50')
                    span.text-decoration-line.text-decoration-line-right(data-caption-animate='widthLines' data-caption-delay='50')
                .group-lg(data-caption-animate='fadeInUp' data-caption-delay='250')
                  a.button.button-primary.button-leaf(href='#') View all events
                  a.button.button-white.button-leaf(href='product-catalog.html') Shop now

      .swiper-slide.bg-overlay-40.context-dark(data-slide-bg='images/slide-2.jpg' style='background-position: 50% 50%')
        .swiper-slide-caption.section-md
          .container
            .row.justify-content-lg-center
              .col-lg-9
                p.slide-title(data-caption-animate='fadeInDown' data-caption-delay='250') Excellent
                h2.text-decoration-lines-2(data-caption-animate='fadeIn' data-caption-delay='50')
                  span Winter holiday opportunities
                    span.text-decoration-line.text-decoration-line-left(data-caption-animate='widthLines' data-caption-delay='50')
                    span.text-decoration-line.text-decoration-line-right(data-caption-animate='widthLines' data-caption-delay='50')
                .group-lg(data-caption-animate='fadeInUp' data-caption-delay='250')
                  a.button.button-primary.button-leaf(href='#') View all events
                  a.button.button-white.button-leaf(href='product-catalog.html') Shop now

      .swiper-slide.bg-overlay-30.context-dark(data-slide-bg='images/slide-3.jpg' style='background-position: 50% 50%')
        .swiper-slide-caption.section-md
          .container
            .row.justify-content-lg-center
              .col-lg-9
                p.slide-title(data-caption-animate='fadeInDown' data-caption-delay='250') Extensive
                h2.text-decoration-lines-2(data-caption-animate='fadeIn' data-caption-delay='50')
                  span Choice of extreme winter sports activities
                    span.text-decoration-line.text-decoration-line-left(data-caption-animate='widthLines' data-caption-delay='50')
                    span.text-decoration-line.text-decoration-line-right(data-caption-animate='widthLines' data-caption-delay='50')
                .group-lg(data-caption-animate='fadeInUp' data-caption-delay='250')
                  a.button.button-primary.button-leaf(href='#') View all events
                  a.button.button-white.button-leaf(href='product-catalog.html') Shop now

    // Swiper Pagination 
    .swiper-pagination
  ="\n"

  // Book your winter holiday
  section.section.section-sm.bg-default.text-md-left.section-transform-top-70
    .container
      .form-booking
        h2 Book your winter holiday
        form.rd-form.rd-mailform.rd-form-outside
          .form-wrap.form-wrap-select
            label.form-label-outside Location
            //Select 2
            select.form-input.select-filter(data-minimum-results-for-search="Infinity", data-constraints="@Required")
              option(value="1") Vermont, Snowqueen Lounge
              option(value="2") Aspen, Buttermilk
              option(value="3") Beaver Creek, Arrowhead

          .form-wrap
            label.form-label-outside Quantity
            input.form-input(type="number" data-zeros="true" value="1" min="1" max="1000")
          .form-wrap.form-wrap-time-picker
            label.form-label-outside Start date
            input.form-input(type="text" data-time-picker="date" data-constraints="@Required" name='departure' readonly)
          .form-wrap.form-wrap-time-picker
            label.form-label-outside End date
            input.form-input(type="text" data-time-picker="date" data-constraints="@Required" name='return' readonly)
          .form-wrap
            a.button.button-primary(href='#') Book now

      .row.row-30.justify-content-center
        .col-sm-8.col-md-6.col-lg-4.col-xl-3.wow.fadeInUp(data-wow-delay='0s')
          +box-info({
            img: 'box-info-1-270x270',
            title: 'Ski',
            text: 'Enjoy the feeling of gliding through the snow in the fresh winter air of your favorite winter mountain trails.'
          })
        .col-sm-8.col-md-6.col-lg-4.col-xl-3.wow.fadeInUp(data-wow-delay='.1s')
          +box-info({
            img: 'box-info-2-270x270',
            title: 'Snowboarding',
            text: 'Have a special winter holiday with a variety of snowboarding activities for adults and children.'
          })
        .col-sm-8.col-md-6.col-lg-4.col-xl-3.wow.fadeInUp(data-wow-delay='.2s')
          +box-info({
            img: 'box-info-3-270x270',
            title: 'Snowmobiles',
            text: 'All winter locations have a variety of snowmobiles available for rent to all our registered customers.'
          })
        .col-sm-8.col-md-6.col-lg-5.col-xl-3.wow.fadeInUp(data-wow-delay='.3s')
          +box-info({
            img: 'box-info-4-270x270',
            title: 'Rock climbing',
            text: 'Climbing rocks in winter is as satisfying as in any other season with lots of benefits from us.'
          })

        .col-lg-7.col-xl-6.wow.fadeInUp(data-wow-delay='0s')
          +box-info({
            img: 'box-info-5-570x400',
            title: 'Popular Resorts',
            text: 'You can easily select the desired winter resort from our extensive online catalog. It is regularly updated with new amazing destinations to provide you with more options. All resorts have 90% of positive reviews from our clients.'
          })

        .col-sm-6.col-md-6.col-lg-4.col-xl-3.wow.fadeInUp(data-wow-delay='.1s')
          +box-product-modern({
            mod: 'box-product-modern',
            badge: true,
            name: 'SCARPA Spirit 3 Alpine Boot',
            price: '59',
            img: 'product-9-180x211'
          })

        .col-sm-6.col-md-6.col-lg-4.col-xl-3.wow.fadeInUp(data-wow-delay='.2s')
          +box-product-modern({
            mod: 'box-product-modern',
            badge: true,
            name: 'Dawn Patrol 25 Pack',
            price: '29',
            img: 'product-10-176x234'
          })
  ="\n"

  // Upcoming events
  section.section.section-sm.section-last.bg-default
    .container
      h1.wow.fadeInUp(data-wow-delay='.1s') Upcoming events
      .row.row-40.justify-content-center.justify-content-md-start
        .col-sm-8.col-md-6.col-lg-4.wow.fadeInUp(data-wow-delay='.1s')
          +post-classic({
            mod: 'post-classic',
            img: 'post-1-370x254',
            title: 'Speed Ice Tour',
            text: 'This event is a true special for all fans of winter and speed. It includes a variety of powerful...',
            timeNumber: '23',
            timeMonth: 'September',
            datetime: '2018-09-23'
          })
        .col-sm-8.col-md-6.col-lg-4.wow.fadeInUp(data-wow-delay='.2s')
          +post-classic({
            mod: 'post-classic',
            img: 'post-2-370x254',
            title: 'Winter Family Activities',
            text: 'We are glad to announce a new series of Winter Sports events for adults and children called...',
            timeNumber: '04',
            timeMonth: 'December',
            datetime: '2018-12-04'
          })
        .col-sm-8.col-md-6.col-lg-4.wow.fadeInUp(data-wow-delay='.3s')
          +post-classic({
            mod: 'post-classic',
            img: 'post-3-370x254',
            title: 'The First Snowmobile Quest',
            text: 'Do you enjoy riding snowmobiles? If you answered “yes”, then we have an amazing offer for you.',
            timeNumber: '12',
            timeMonth: 'January',
            datetime: '2018-01-12'
          })

      a.button.button-primary.button-leaf.wow.fadeInUp(href='classic-blog.html' data-wow-delay='.1s') View all events
  ="\n"

  include ../sections/_section-counters

  section.section.section-xxl.bg-default.text-md-left
    .container
      .row.row-40.justify-content-xl-between.align-items-center.align-items-xl-start
        .col-md-8.col-lg-6.wow.fadeInUp(data-wow-delay='0s')
          a.video-cover.bg-overlay-30(data-lightbox='iframe', href='https://www.youtube.com/watch?v=1UWpbtUupQQ')
            +jpg('video-1-570x381')
            span.icon.mdi.mdi-play.video-cover-icon

        .col-lg-6.col-xl-5
          h1.wow.fadeInUp(data-wow-delay='.1s') About us
          p.big.wow.fadeInUp(data-wow-delay='.15s') We are a community of friendly people who love all kinds of winter sports & activities.
          p.wow.fadeInUp(data-wow-delay='.2s') Our team unites current & former snowboarders, skiers, bloggers and journalists specializing in writing about all kinds of winter activities and holiday opportunities for everyone. We also provide a wide range of skiing and snowboarding equipment and apparel to all our visitors and customers. Feel free to become one of them!
          a.button.button-primary.button-leaf.wow.fadeInUp(href='about.html' data-wow-delay='.25s') View all information
  ="\n"

  // Our shop
  section.section.section-xxl.bg-white
    .container
      h1.wow.fadeInUp(data-wow-delay='0s') Our shop
      .row.row-50.row-lg-80
        include ../sections/_section-product-catalog-2-line
  ="\n"

  // Latest blog posts
  section.section.section-xxl.bg-default
    .container
      h1.wow.fadeInUp(data-wow-delay='0s') Latest blog posts
      .row.row-40.justify-content-center.justify-content-md-start
        .col-sm-8.col-md-6.col-lg-4.wow.fadeInUp(data-wow-delay='.1s')
          +post-modern({
            mod: 'post-modern post-height',
            img: 'post-4-370x300',
            title: '10 New Snow Sports to Try This Winter',
            text: 'From the tame to the exotic, these alternative winter sports will keep you outside and in shape until all the snow...',
            time: 'December 20, 2018 at 12:29 pm',
            datetime: '2018-12-20 12:29',
            badge: 'tips & tricks'
          })
        .col-sm-8.col-md-6.col-lg-4.wow.fadeInUp(data-wow-delay='.2s')
          +post-modern({
            mod: 'post-modern post-height',
            img: 'post-5-370x300',
            title: 'Daredevil Winter Sports in Finland',
            text: 'Finland is perhaps best known for the Aurora Borealis, but there’s a lot more, especially for sporting daredevils. With its frozen waters...',
            time: 'January 20, 2018 at 13:29 pm',
            datetime: '2018-01-20 13:29',
            badge: 'articles'
          })
        .col-sm-8.col-md-6.col-lg-4.wow.fadeInUp(data-wow-delay='.3s')
          +post-modern({
            mod: 'post-modern post-height',
            img: 'post-6-370x300',
            title: 'Best-Value Family Ski Trips',
            text: 'Winter sports breaks can be seriously expensive during February half-term and the Easter holidays but there are bargains if...',
            time: 'February 20, 2018 at 14:29 pm',
            datetime: '2018-02-20 14:29',
            badge: 'skiing'
          })

      a.button.button-primary.button-leaf.wow.fadeInUp(href='classic-blog.html' data-wow-delay='0s') View all blog posts
  ="\n"

  include ../sections/_section-subscribe

