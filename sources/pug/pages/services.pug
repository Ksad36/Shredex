extends  ../_skeleton

//- Includes
include ../components/_box-minimal-horizontal
include ../components/_quote

block variables
  - var activeCategory = 'pages'
  - var pageName = 'Services'

block page 
  // Page Header
  include ../sections/_header-classic

  +breadcrumbs({
    category: activeCategory,
    subtitle: 'What We Offer',
    path: [pageName],
    bgImage: 'bg-breadcrumbs'
  })

  // About Services
  section.section.section-xxl.bg-default.text-md-left
    .container
      .row.row-40.flex-lg-row-reverse.justify-content-xl-between.align-items-center.align-items-xl-start
        .col-md-8.col-lg-6
          +png('services-1-570x412')
        .col-lg-6.col-xl-5
          h1 About Services
          p.big Lorem ipsum dolor sit amet, et est labore labitur nominavi, melius perfecto petentium qui eu.
          p Lorem ipsum dolor sit amet, et est labore labitur nominavi, melius perfecto petentium qui eu. Tibique accommodare vis ex, nec nonumes probatus elaboraret id. Elitr docendi nam id. Libris habemus reprimique at mel, nam in errem epicuri constituam.
          a.button.button-primary.button-leaf(href='#') View all information
  ="\n"

